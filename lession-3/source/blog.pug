include modules/head
include modules/header
include modules/footer
include modules/script
include modules/components/blog
include modules/components/breadcumb
include modules/components/section-head

doctype html
html
  +head
  body
    +header
    +breadcumb("Home", "Blog")
    main.blogs
      .container
        h1.blogs__title BLOG
        +section-head
        .row.blogs__list
          each item in [1, 2, 3, 4, 5, 6 ,7 ,8 ,9]
            .col-lg-4.col-md-6.blogs__blog
              +blog("images/blog-"+item+".png")
        +section-head
    +footer
    +script
